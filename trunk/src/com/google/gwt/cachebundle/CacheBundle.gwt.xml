<!-- CacheBundle supports renaming files with strong names at compile time  -->
<module>
  <inherits name="com.google.gwt.core.Core" />
  <generate-with
    class="com.google.gwt.cachebundle.rebind.CacheBundleGenerator">
    <when-type-assignable class="com.google.gwt.cachebundle.client.CacheBundle" />
  </generate-with>

  <generate-with
    class="com.google.gwt.cachebundle.rebind.InlineCacheBundleGenerator">
    <all>
      <when-type-assignable
        class="com.google.gwt.cachebundle.client.CacheBundle" />
      <any>
        <when-property-is name="user.agent" value="safari" />
        <when-property-is name="user.agent" value="opera" />
        <when-property-is name="user.agent" value="gecko" />
        <when-property-is name="user.agent" value="gecko1_8" />
      </any>
    </all>
  </generate-with>
</module>